<?xml version="1.0" encoding="utf-8"?>
<AutoVisualizer xmlns="http://schemas.microsoft.com/vstudio/debugger/natvis/2010">

    <Type Name="skr::text::index_interval">
        <DisplayString Condition="lower_.type == 2 &amp; upper_.type == 2">
            (-∞,+∞)
        </DisplayString>
        <DisplayString Condition="lower_.type == 2 &amp; upper_.type == 1">
            (-∞,{upper_.value})
        </DisplayString>
        <DisplayString Condition="lower_.type == 2 &amp; upper_.type == 0">
            (-∞,{upper_.value}]
        </DisplayString>
        <DisplayString Condition="lower_.type == 1 &amp; upper_.type == 2">
            ({lower_.value},+∞)
        </DisplayString>
        <DisplayString Condition="lower_.type == 0 &amp; upper_.type == 2">
            [{lower_.value},+∞)
        </DisplayString>
        <DisplayString Condition="lower_.type == 1 &amp; upper_.type == 1">
            ({lower_.value},{upper_.value})
        </DisplayString>
        <DisplayString Condition="lower_.type == 1 &amp; upper_.type == 0">
            ({lower_.value},{upper_.value}]
        </DisplayString>
        <DisplayString Condition="lower_.type == 0 &amp; upper_.type == 1">
            [{lower_.value},{upper_.value})
        </DisplayString>
        <DisplayString Condition="lower_.type == 0 &amp; upper_.type == 0">
            [{lower_.value},{upper_.value}]
        </DisplayString>
        <DisplayString>
            invalid
        </DisplayString>
    </Type>

    <Type Name="skr::text::codeunit_sequence_view">
        <DisplayString>{data_,[size_]}</DisplayString>
        <Expand HideRawView="true">
            <Item Name="size">size_</Item>
        </Expand>
    </Type>

    <Type Name="skr::text::codeunit_sequence">
        <DisplayString>{((skr::text::codeunit_sequence::sso*)this)->alloc ? ((skr::text::codeunit_sequence::norm*)this)->data : ((skr::text::codeunit_sequence::sso*)this)->data._Elems}</DisplayString>
        <Expand HideRawView="true">
            <Item Name="allocated">(bool)(((skr::text::codeunit_sequence::sso*)this)->alloc)</Item>
            <Item Name="size">((skr::text::codeunit_sequence::sso*)this)->alloc ? ((skr::text::codeunit_sequence::norm*)this)->size : (skr::text::u32)((skr::text::codeunit_sequence::sso*)this)->size</Item>
            <Item Name="capacity">((skr::text::codeunit_sequence::sso*)this)->alloc ? ((skr::text::codeunit_sequence::norm*)this)->capacity : skr::text::codeunit_sequence::SSO_SIZE_MAX</Item>
            <Item Name="data">((skr::text::codeunit_sequence::sso*)this)->alloc ? ((skr::text::codeunit_sequence::norm*)this)->data : (skr::text::u32)((skr::text::codeunit_sequence::sso*)this)->data._Elems</Item>
        </Expand>
    </Type>

</AutoVisualizer>